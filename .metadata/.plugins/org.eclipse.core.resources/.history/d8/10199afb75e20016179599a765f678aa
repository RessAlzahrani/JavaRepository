package namedConstants1;

/**
 * @author RA316282
 *
 */
public class namedConstants1 {

	/**
	 * @param args
	 */
	public static void main(String[] args) 
	{
		int first_number;
		int second_number;
		int third_number;
		int fourth_number;
		int answer;
		first_number = 100;
		second_number = 75;
		third_number = 20;
		fourth_number = 4;  
		answer = (first_number + second_number + third_number) ;
		
		System.out.println("Total =" + answer);

	}

}
